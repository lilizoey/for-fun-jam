; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "BookEntry",
"language": "GDScript",
"path": "res://Database/BookEntry.gd"
}, {
"base": "Node",
"class": "DragAndDropper",
"language": "GDScript",
"path": "res://DragAndDrop/DragAndDropper.gd"
}, {
"base": "Reference",
"class": "Ingredient",
"language": "GDScript",
"path": "res://Database/Ingredient.gd"
}, {
"base": "Reference",
"class": "Potion",
"language": "GDScript",
"path": "res://Database/Potion.gd"
}, {
"base": "Reference",
"class": "Recipe",
"language": "GDScript",
"path": "res://Database/Recipe.gd"
} ]
_global_script_class_icons={
"BookEntry": "",
"DragAndDropper": "",
"Ingredient": "",
"Potion": "",
"Recipe": ""
}

[application]

config/name="for-fun-jam"
run/main_scene="res://Main.tscn"

[autoload]

DatabaseConstants="*res://Database/DatabaseConstants.gd"

[display]

window/size/width=1920
window/size/height=1080

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
